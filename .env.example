# NyaySetu .env.example
# Copy this file to .env and fill in your values

# Server Configuration
PORT=8080

# Bhashini API Keys (Default values included for development)
BHASHINI_UDYAT_KEY=
BHASHINI_INFERENCE_API_KEY=
BHASHINI_API_URL=https://bhashini.gov.in/api

# Azure Cognitive Services (Required for Azure integration)
# Azure Translator Service
AZURE_TRANSLATOR_KEY=
AZURE_TRANSLATOR_REGION=eastus
AZURE_TRANSLATOR_ENDPOINT=https://api.cognitive.microsofttranslator.com/

# Azure Text Analytics
AZURE_TEXT_ANALYTICS_KEY=
AZURE_TEXT_ANALYTICS_ENDPOINT=https://your-resource-name.cognitiveservices.azure.com/

# Azure Speech Service
AZURE_SPEECH_KEY=
AZURE_SPEECH_ENDPOINT=https://eastus.api.cognitive.microsoft.com/
AZURE_SPEECH_REGION=eastus

# Azure OpenAI Service
AZURE_OPENAI_KEY=
AZURE_OPENAI_ENDPOINT=https://brahmacoders.openai.azure.com/
AZURE_OPENAI_REGION=eastus

# Google Cloud (Optional, used as final fallback)
# GOOGLE_APPLICATION_CREDENTIALS=path/to/your/google-credentials.json
